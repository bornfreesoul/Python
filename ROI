__author__ = 'testing-mines.blogspot.in'

#ROI = (Profit - Investment) / Investment

print "*** Welcome to the ROI Application ***"

profit = float(raw_input("Please enter the profit: \n"));
investment = float(raw_input("Please enter the investment: \n"));

ROI = float((profit - investment)/investment);

ROI2 = ROI * 100

print "The rate of investment (ROI) is: ", ROI2;

import webbrowser

webbrowser.open("http://testing-mines.blogspot.in/");
webbrowser.open("http://net-mines.blogspot.in/");
