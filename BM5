word = raw_input("Enter your word: \n"); 
print word;

if len(word) < 5:
   print "Your word length is less than 5"
elif len(word) == 5:
   print "Your word length is equal to 5"
   if word == "TonyJ":
      print "Hi TonyJ!";
else:
   print "Your word length is equal or greater than 5"

List = ["One","Two","Three"];
print List;
print List[2];

List[2] = "Change";
print List[2];
print List;

List.append("Three");
print List;

List.insert(1, "THREE");
print List;

List2 = ["Five", "Six"];
print List2;

List.extend(List2);
print List;

List.remove("Five");
print List;

