cars = ["Honda", "Maruti", "Tata", "Fiat","Honda"];
autos = set(cars);
print autos;

motorcyles = ["Honda","KWS", "Harley","Trump","Yamaha","BMW"];
moto = set(motorcyles)
print moto;

print autos - moto;

